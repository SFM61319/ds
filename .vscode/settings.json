{
  "editor.rulers": [80],
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "C_Cpp.autocomplete": "default",
  "C_Cpp.clang_format_fallbackStyle": "GNU",
  "C_Cpp.clang_format_style": "file",
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "C_Cpp.codeAnalysis.clangTidy.config": "",
  "C_Cpp.codeAnalysis.clangTidy.codeAction.formatFixes": true,
  "C_Cpp.codeAnalysis.clangTidy.useBuildPath": true,
  "C_Cpp.commentContinuationPatterns": ["//!", "///"],
  "C_Cpp.doxygen.generatedStyle": "///",
  "C_Cpp.doxygen.generateOnType": true,
  "C_Cpp.enhancedColorization": "enabled",
  "C_Cpp.errorSquiggles": "enabledIfIncludesResolve",
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.intelliSenseEngine": "default",
  "C_Cpp.preferredPathSeparator": "Forward Slash",
  "C_Cpp.suggestSnippets": true,
  "prettier.requireConfig": true,
  "prettier.configPath": ".prettierrc.json",
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.associations": {
    ".clang-format": "yaml",
    ".clang-tidy": "yaml",
    ".clangd": "yaml",
    "Doxyfile": "ini",
    "LICENSE": "plaintext",
    "*.c": "c",
    "*.c.in": "c",
    "*.cpp": "cpp",
    "*.cpp.in": "cpp",
    "*.h": "c",
    "*.h.in": "c",
    "*.hpp": "cpp",
    "*.hpp.in": "cpp",
    "*.cmake.in": "cmake",
    "*.exe": "hexEditor.hexedit",
    "*.o": "hexEditor.hexedit",
    "*.dll": "hexEditor.hexedit",
    "*.pdb": "hexEditor.hexedit",
    "*.swn": "hexEditor.hexedit",
    "*.swo": "hexEditor.hexedit",
    "*.emz": "hexEditor.hexedit",
    "*.idx": "hexEditor.hexedit",
    "*.gcda": "hexEditor.hexedit",
    "*.a": "hexEditor.hexedit",
    "*.so": "hexEditor.hexedit",
    "*.so.*": "hexEditor.hexedit",
    "*.ko": "hexEditor.hexedit",
    "*.obj": "hexEditor.hexedit",
    "*.elf": "hexEditor.hexedit",
    "*.gch": "hexEditor.hexedit",
    "*.pch": "hexEditor.hexedit",
    "*.lib": "hexEditor.hexedit",
    "*.la": "hexEditor.hexedit",
    "*.lai": "hexEditor.hexedit",
    "*.lo": "hexEditor.hexedit",
    "*.slo": "hexEditor.hexedit",
    "*.dylib": "hexEditor.hexedit",
    "*.out": "hexEditor.hexedit",
    "*.app": "hexEditor.hexedit",
    "*.i*86": "hexEditor.hexedit",
    "*.x86_64": "hexEditor.hexedit",
    "*.hex": "hexEditor.hexedit",
    "*.su": "hexEditor.hexedit",
    "*.idb": "hexEditor.hexedit"
  },
  "yaml.disableAdditionalProperties": false,
  "yaml.disableDefaultProperties": false,
  "yaml.validate": true,
  "yaml.yamlVersion": "1.2",
  "yaml.schemas": {
    "https://json.schemastore.org/github-issue-config.json": "${workspaceFolder}/.github/ISSUE_TEMPLATE/config.yml"
  },
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  }
}
